{
  "compilerOptions": {
      "module": "ES2015",
      "target": "ES2015",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "outDir": "./app/cache",
      // "outFile": "../../built/local/tsc.js",
      "sourceMap": true,
      "allowSyntheticDefaultImports":true
  },
  "include": [
  "app/cache/**/*.ts"
  ],
  "exclude": [
  "app/cache/**/*.js"
  ]
}
